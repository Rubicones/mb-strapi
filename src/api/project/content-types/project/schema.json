{
  "kind": "collectionType",
  "collectionName": "projects",
  "info": {
    "singularName": "project",
    "pluralName": "projects",
    "displayName": "Project"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "Name": {
      "type": "string"
    },
    "Description": {
      "type": "text"
    },
    "Date": {
      "type": "date"
    },
    "isPosted": {
      "type": "boolean"
    },
    "Cover": {
      "type": "media",
      "multiple": false,
      "allowedTypes": [
        "images",
        "files",
        "videos",
        "audios"
      ]
    },
    "Content": {
      "type": "enumeration",
      "enum": [
        "youtube",
        "spline",
        "none"
      ]
    },
    "Programs": {
      "type": "component",
      "component": "project.program",
      "repeatable": true
    },
    "Media": {
      "type": "component",
      "component": "project.media",
      "repeatable": true
    }
  }
}
