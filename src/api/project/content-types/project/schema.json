{
  "kind": "collectionType",
  "collectionName": "projects",
  "info": {
    "singularName": "project",
    "pluralName": "projects",
    "displayName": "Project"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "Name": {
      "type": "string",
      "required": false
    },
    "Description": {
      "type": "text"
    },
    "Date": {
      "type": "date"
    },
    "isPosted": {
      "type": "boolean"
    },
    "Cover": {
      "type": "media",
      "multiple": false,
      "allowedTypes": [
        "images",
        "files",
        "videos",
        "audios"
      ]
    },
    "Content": {
      "type": "enumeration",
      "enum": [
        "youtube",
        "spline",
        "none"
      ]
    },
    "Media": {
      "type": "component",
      "component": "project.media",
      "repeatable": true
    },
    "Programs": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::program.program"
    },
    "SplineLink": {
      "type": "text"
    },
    "ytLink": {
      "type": "text"
    },
    "Category": {
      "type": "enumeration",
      "enum": [
        "c_3D",
        "c_2D",
        "c_Craft"
      ]
    },
    "nextProject": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::project.project"
    },
    "prevProject": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::project.project"
    },
    "isHighlighted": {
      "type": "boolean"
    }
  }
}
